{
  "name": "js-datepicker",
  "version": "5.18.2",
  "description": "Get a date with JavaScript! A datepicker with no dependencies.",
  "main": "dist/datepicker.js",
  "module": "dist/datepicker.mjs",
  "types": "dist/datepicker.d.ts",
  "style": "./src/datepicker.scss",
  "scripts": {
    "build": "vite build && npm run build:scss",
    "build:scss": "sass src/datepicker.scss dist/datepicker.css --style=compressed --no-source-map",
    "cy:open": "cypress open --e2e -b chrome",
    "cy:run": "cypress run",
    "dev": "vite",
    "dev-test": "vite --mode test",
    "test": "node runTests.js --type run",
    "test:open": "node runTests.js --type open",
    "tsc": "tsc --project tsconfig.json --noemit",
    "tsc:test": "tsc --project ./cypress/tsconfig.json --noemit",
    "tsc:all": "npm run tsc && npm run tsc:test"
  },
  "keywords": [
    "datepicker",
    "date picker",
    "daterange picker",
    "date range picker",
    "date range",
    "daterange",
    "date",
    "picker",
    "calendar"
  ],
  "author": "Qodesmith",
  "email": "theqodesmith@gmail.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/qodesmith/datepicker"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.15.13",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "cypress": "^12.10.0",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "eslint": "^8.39.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recoil": "^0.7.7",
    "sass": "^1.62.0",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.1",
    "vite-plugin-dts": "^2.3.0"
  }
}
