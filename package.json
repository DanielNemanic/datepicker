{
  "name": "js-datepicker",
  "version": "5.18.2",
  "description": "Get a date with JavaScript! A datepicker with no dependencies.",
  "main": "dist/datepicker.min.js",
  "style": "./src/datepicker.scss",
  "scripts": {
    "build": "vite build",
    "cy:open": "cypress open --e2e -b chrome",
    "cy:run": "cypress run",
    "dev": "vite",
    "dev-test": "vite --mode test",
    "test": "node runTests.js --type run",
    "test:open": "node runTests.js --type open",
    "tsc": "tsc --project tsconfig.json --noemit",
    "tsc:test": "tsc --project ./cypress/tsconfig.json --noemit",
    "tsc:all": "npm run tsc && npm run tsc:test"
  },
  "keywords": [
    "datepicker",
    "date picker",
    "daterange picker",
    "date range picker",
    "date range",
    "daterange",
    "date",
    "picker",
    "calendar"
  ],
  "author": "Qodesmith",
  "email": "theqodesmith@gmail.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/qodesmith/datepicker"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.0.1",
    "cypress": "^12.5.1",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "eslint": "^8.31.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recoil": "^0.7.6",
    "sass": "^1.57.1",
    "start-server-and-test": "^1.15.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  }
}
